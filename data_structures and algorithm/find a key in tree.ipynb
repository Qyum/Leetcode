   "source": [
    "def searchkey(root,key):\n",
    "    if root is None:\n",
    "        return root\n",
    "    if root.val==key:\n",
    "        return root\n",
    "    if root.val<key:\n",
    "        return searchkey(root.right,key)\n",
    "    \n",
    "    return searchkey(root.left,key)"
   ]
  }
